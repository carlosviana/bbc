<h2>Listagem de Usu√°rios</h2>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>E-mail</th>
      <th>Telefone</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% for user in @users %>
      <tr>
        <td><%= user.nome %></td>
        <td><%= user.email %></td>
        <td><%= user.telefone %></td>
        <td><%= link_to "#{user.enabled}", user_enabled_path(user.id) %></td>
        <td><%= link_to 'Ver', user %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

